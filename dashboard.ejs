         <div id="TicketIncident" class="tabcontent">
            <h3>Ticket / Incident</h3>
            <form id="IncidentManagementForm" action="/incident" method="POST">
               <div class="row">
                  <div class="col">
                     <div class="row">
                        <div class="col-25">
                           <label for="incidentid">Incident ID</label>
                        </div>
                        <div class="col-75">
                           <% if(messages.success_msg) { %>
                           <input type="text" id="incidentidval" name="incidentidval" placeholder="Incident ID.." value="<%= messages.success_msg %>">
                           <% } else if(searchResult.length > 0) {%>
                           <input type="text" id="incidentidval" name="incidentidval" placeholder="Incident ID.." value="<%= searchResult[0].ticket_no %>">
                           <% } else { %>
                           <input type="text" id="incidentidval" name="incidentidval" placeholder="Incident ID.." value="">
                           <% } %>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="companyname">Company Name</label>
                        </div>
                        <div class="col-75">
                           <% if(searchResult.length > 0) { %>
                           <input type="text" id="companyname" name="companyname" placeholder="Company name.." value="<%= searchResult[0].company_name %>">
                           <%} else {%>	
                           <input type="text" id="companyname" name="companyname" placeholder="Company name.." value="">
                           <% } %>	
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="companynum">Company Phone.</label>
                        </div>
                        <div class="col-75">
                           <% if(searchResult.length > 0) { %>
                           <input type="text" id="companynum" name="companynum" placeholder="Company Phone.." value="<%= searchResult[0].company_phone %>">
                           <% } else { %>
                           <input type="text" id="companynum" name="companynum" placeholder="Company Phone.." value="">
                           <% } %>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="companyadd">Company Address</label>
                        </div>
                        <div class="col-75">
                           <% if(searchResult.length > 0) { %>				    	
                           <input type="text" id="companyadd" name="companyadd" placeholder="Company Address.." value="<%= searchResult[0].company_address %>">
                           <% } else { %>
                           <input type="text" id="companyadd" name="companyadd" placeholder="Company Address.." value="">
                           <% } %>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="reportdate">Report Date</label>
                        </div>
                        <div class="col-75">
                           <% if(searchResult.length > 0) { %>				    	
                           <input type="date" id="reportdate" name="reportdate" placeholder="Report Date.." value="<%= moment(searchResult[0].reported_date).format('YYYY-MM-DD') %>">
                           <% } else { %>
                           <input type="date" id="reportdate" name="reportdate" placeholder="Report Date..">
                           <% } %>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="reportedby">Reported By</label>
                        </div>
                        <div class="col-75">
                           <% if(searchResult.length > 0) { %>				    	
                           <input type="text" id="reportedby" name="reportedby" placeholder="Reported By.." value="<%= searchResult[0].reported_by %>">
                           <% } else { %>
                           <input type="text" id="reportedby" name="reportedby" placeholder="Reported By..">				      
                           <% } %>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="assignedto">Assigned To</label>
                        </div>
                        <div class="col-75">
                           <select name="assignedto" id="assignedto">
                              <% if(searchResult.length > 0) { %>
                              <option value="<%= searchResult[0].assigned_to %>" name="<%= searchResult[0].assigned_to %>"><%= searchResult[0].assigned_to %></option>
                              <% for(var i = 0;i < ddAssgnUsersVals.length; i++) { %>
                              <% if(searchResult[0].assigned_to !== ddAssgnUsersVals[i].username) {%>
                              <option value="<%= ddAssgnUsersVals[i].username %>" name="<%= ddAssgnUsersVals[i].username %>"><%= ddAssgnUsersVals[i].username %></option>
                              <% } %>
                              <% } %>
                              <% } else {%>
                              <% for(var i = 0;i < ddAssgnUsersVals.length; i++) { %>
                              <option value="<%= ddAssgnUsersVals[i].username %>" name="<%= ddAssgnUsersVals[i].username %>"><%= ddAssgnUsersVals[i].username %></option>
                              <% } %>
                              <% } %>
                           </select>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="clientdepartment">Client / Department</label>
                        </div>
                        <div class="col-75">
                           <% if(searchResult.length > 0) { %>				    	
                           <input type="text" id="clientdepartment" name="clientdepartment" placeholder="Client / Department.." value="<%= searchResult[0].department_name %>">
                           <% } else { %>		
                           <input type="text" id="clientdepartment" name="clientdepartment" placeholder="Client / Department.." value=""> 
                           <% } %>
                        </div>
                     </div>
                     <strong>Incident Details:</strong> <br>
                     <div class="row">
                        <div class="col-25">
                           <label for="dateandtime">Date and Time</label>
                        </div>
                        <div class="col-75">
                           <% if(searchResult.length > 0) { %>   	
                           <input type="datetime-local" id="dateandtime" name="dateandtime" placeholder="Date and Time.." value="<%= moment(searchResult[0].incident_date).format('YYYY-MM-DD HH:mm:ss') %>">
                           <% } else { %>		
                           <input type="datetime-local" id="dateandtime" name="dateandtime" placeholder="Date and Time.." value="">
                           <% } %>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="impactedservices">Impacted Services</label>
                        </div>
                        <div class="col-75">
                           <select name="impactedservices" id="impactedservices">
                              <% if(searchResult.length > 0) { %>
                              <option value="<%= searchResult[0].impacted_services %>" name="<%= searchResult[0].impacted_services %>"><%= searchResult[0].impacted_services %></option>
                              <% } %>
                              <% for(var i = 0;i < ddImpctdSrvcVals.length;i++) { %>
                              <% if(searchResult[0].impacted_services !== ddImpctdSrvcVals[i].impactedservice) {%>
                              <option value="<%= ddImpctdSrvcVals[i].impactedservice %>" name="<%= ddImpctdSrvcVals[i].impactedservice %>"><%= ddImpctdSrvcVals[i].impactedservice %></option>
                              <% } %>
                              <% } %>
                           </select>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="servicelevelagreement">Service Level Agreement</label>
                        </div>
                        <div class="col-75">
                           <% if(searchResult.length > 0) { %>				    	
                           <input type="text" id="servicelevelagreement" name="servicelevelagreement" placeholder="Service Level Agreement.." value="<%= searchResult[0].servicelevelagreement %>">
                           <% } else { %>
                           <input type="text" id="servicelevelagreement" name="servicelevelagreement" placeholder="Service Level Agreement.." value="">
                           <% } %>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="incidenttype">Incident Type</label>
                        </div>
                        <div class="col-75">
                           <select name="incidenttype" id="incidenttype">
                              <% if(searchResult.length > 0) { %>
                              <option value="<%= searchResult[0].incident_type %>" name="<%= searchResult[0].incident_type %>"><%= searchResult[0].incident_type %></option>
                              <% } %>
                              <% for(var i = 0;i < ddIncidentType.length; i++) { %>
                              <% if(searchResult[0].incident_type !== ddIncidentType[i].incidenttype) {%>
                              <option value="<%= ddIncidentType[i].incidenttype %>" name="<%= ddIncidentType[i].incidenttype %>"><%= ddIncidentType[i].incidenttype %></option>
                              <% } %>
                              <% } %>
                           </select>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="incidentseverty">Severity</label>
                        </div>
                        <div class="col-75">
                           <select name="severity" id="severity">
                              <% if(searchResult.length > 0) { %>
                              <option value="<%= searchResult[0].severity %>" name="<%= searchResult[0].severity %>"><%= searchResult[0].severity %></option>
                              <% } %>
                              <% for(var i = 0;i < ddIncidentSvrty.length; i++) { %>
                              <% if (searchResult[0].severity !== ddIncidentSvrty[i].incidentseverity) {%>
                              <option value="<%= ddIncidentSvrty[i].incidentseverity %>" name="<%= ddIncidentSvrty[i].incidentseverity %>"><%= ddIncidentSvrty[i].incidentseverity %></option>
                              <% } %>
                              <% } %>
                           </select>
                        </div>
                     </div>
                     <div class="row">
                        <div class="col-25">
                           <label for="status">Status</label>
                        </div>
                        <div class="col-75">
                           <select name="status" class="status" id="status">
                              <% if(searchResult.length > 0) { %>
                              <option value="<%= searchResult[0].status %>" name="<%= searchResult[0].status %>"><%= searchResult[0].status %></option>
                              <% } %>
                              <% for(var i = 0;i < ddIncidentStatus.length; i++) { %>
                              <% if (searchResult[0].status !== ddIncidentStatus[i].incidentstatus ) {%>
                              <option value="<%= ddIncidentStatus[i].incidentstatus %>" name="<%= ddIncidentStatus[i].incidentstatus %>"><%= ddIncidentStatus[i].incidentstatus %></option>
                              <% } %>
                              <% } %>
                           </select>
                        </div>
                     </div>
                  </div>
                  <div class="col">
                     <div class="row">
                        <div class="col" style="color: #615F1D;">
                           <label for="incidentsummary">Incident Summary</label>
                        </div>
                        <div class="col">
                           <% if(searchResult.length > 0) { %>
                           <textarea id="incidentsummary" name="incidentsummary" rows="4" columns="50" style="height: 100px; width: 400px" value="<%= searchResult[0].incident_summary %>"><%= searchResult[0].incident_summary %></textarea>
                           <% } else { %>
                           <textarea id="incidentsummary" name="incidentsummary" rows="4" columns="50" style="height: 100px; width: 400px" ></textarea>
                           <% } %>	
                        </div>
                     </div>
                     <div class="row">
                        <div class="col" style="color: #615F1D; margin-top: 10px">
                           <label for="impact">Impact</label>
                        </div>
                        <div class="col">
                           <% if(searchResult.length > 0) { %>				    	
                           <textarea id="impact" name="impact" rows="4" columns="50" style="height: 100px; width: 400px; margin-top: 10px" value="<%= searchResult[0].impact %>"><%= searchResult[0].impact %></textarea>
                           <%} else {%>
                           <textarea id="impact" name="impact" rows="4" columns="50" style="height: 100px; width: 400px; margin-top: 10px"></textarea>		
                           <% } %>	
                        </div>
                     </div>
                     <div class="row">
                        <div class="col" style="margin-top: 20px; color: #615F1D;">
                           <label for="filebrowse">Attachment</label>
                        </div>
                        <div class="col" style="margin-top: 20px;">
                           <% if(searchResult.length > 0) { %>	
                           <label class="file"><%= searchResult[0].attachement_url %></label>
                           <%} else {%>
                           <label class="file">
                           <input type="file" />
                           </label>
                           <% } %>
                        </div>
                     </div>
                  </div>
               </div>
               <div class="row" class="buttons" id="buttons">
                  <div class="col-button">
                     <!-- <input type="Submit" id="save" name="save" Value="SAVE" onclick="redirectToSection('TicketIncident')"> -->
                     <input type="Submit" id="save" name="save" Value="SAVE">
                     <!-- <input type="Submit" id="submit" name="submit" Value="SUBMIT">
                        <input type="Submit" id="cancel" name="cancel" Value="CANCEL">
                        <input type="Submit" id="close" name="close" Value="CLOSE"> -->
                     <input type="Submit" id="clear" name="clear" Value="CLEAR">
                     <input type="Submit" id="searchticket" name="searchticket" Value="SEARCH">
                  </div>
               </div>
            </form>
         </div>
         <div id="Change Request" class="tabcontent">
